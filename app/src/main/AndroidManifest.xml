<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vincent.acnt">

    <application
        android:name=".data.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SubjectActivity" />
        <activity android:name=".JournalActivity" />
        <activity android:name=".EntryCreateActivity" />
        <activity android:name=".EntryUpdateActivity" />
        <activity android:name=".EntryDetailActivity" />
        <activity android:name=".LedgerActivity" />
        <activity android:name=".ReportActivity"></activity>
    </application>

</manifest>